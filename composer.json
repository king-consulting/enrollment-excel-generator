{
  "name": "king-consulting/enrollment-excel-generator",
  "type" : "project",
  "homepage": "https://github.com/king-consulting/enrollment-excel-generator",
  "description": "Generate Enrollment Excel Files",
  "minimum-stability": "dev",
  "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:king-consulting/enrollment-orm.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "KingConsulting\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "require": {
    "king-consulting/enrollment-orm": "master@dev",
    "adldap/adldap": "5.0.0.x-dev as 5.0.0.0",
    "guzzlehttp/guzzle": "^6.2",
    "aws/aws-sdk-php": "3.*",
    "kevinrob/guzzle-cache-middleware": "^1.5",
    "league/container": "^2.2",
    "monolog/monolog": "^1.15",
    "slim/slim": "^3.4",
    "slim/twig-view": "^2.1",
    "twig/twig": "^1.31",
    "doctrine/orm": "^2.6",
    "symfony/yaml": "^4.1",
    "phpoffice/phpexcel": "^1.8"
  },
  "suggest": {
    "ext-redis": "For caching Doctrine proxies"
  }
}
